	/delete-property/ qcom,stacked-batfet;					/* def:N (NON_STACKED_BATFET) */
	///delete-property/ qcom,hw-die-temp-mitigation;			/* def:N */
	//qcom,die-temp-threshold-degc = <90>;					/* def:90 (C) */
