/* Battery Data */
/delete-node/ &mtp_batterydata;

&vendor {
	fih_batterydata: qcom,battery-data {
		qcom,batt-id-range-pct = <15>;					/* qti:15 */
		#include "fg-gen3-batterydata-scud-3320mah-aop.dtsi"
	};
};

